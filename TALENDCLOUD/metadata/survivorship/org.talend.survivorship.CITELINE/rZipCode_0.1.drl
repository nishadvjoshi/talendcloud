package org.talend.survivorship.CITELINE 

rule "rZipCode"
    no-loop true
    dialect "mvel"
    ruleflow-group "rZipCodeGroup"
when
    $input : RecordIn( dataset.isLongest( zipcode, "ZipCode", false) )  
then
    dataset.survive( $input.TALEND_INTERNAL_ID, "ZipCode", drools.getRule().getName());

end


